{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "dist" }
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      {
        "src": "/clientes/cencosud/document-mongo(?:/.*)?",
        "dest": "/index.html"
      }
    ]
  }