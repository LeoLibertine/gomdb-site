{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    { "handle": "filesystem" },

    {
      "src": "/clientes/kushki/mf(?:/.*)?$",
      "dest": "/clientes/kushki/mf.html"
    },
    {
      "src": "/clientes/(?:cencosud|bancolombia)/document-mongo(?:/.*)?$",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}